def main()
{
    val mutable a = int32();
    print_int(a);
    val mutable aa = a + 1;
    val mutable b = int32(aa);
    print_int(b);

    b = 10;
    print_int(b);

    b = b + b + b;
    print_int(b);

    // cannot assign a value to the immutable val
    val yo = 0;
    assert_not(__statement_traits(semantics, {
        yo = 10;
    }));
}
