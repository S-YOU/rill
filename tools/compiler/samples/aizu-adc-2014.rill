def main(): int {
    val i = "i";
    ref calendar = "Calendar";
    (Aizu() + i).f("u").advent(calendar)();
    return 0;
}

class Aizu {
    def ctor() {
        p( "A" );
    }

    def op +(v: ptr!int8) {
        p(v);
        return u("z");
    }
}

class u {
    def ctor(v: ptr!int8) {
        this.f(v);
    }

    def f(v: ptr!int8): ref(u) {
        p(v);
        return this;
    }
}

def advent!(T: type)(_: u, c: T) {
    p("Advent");
    return \() => p(c);
}