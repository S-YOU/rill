def main(): int
{
    val a: mutable(int) = 81;
    f( a );
    g( a );
    val b: int = 81;
    h( b );
    i( a );
    j( a );
    k( a );
    return 0;
}

def f(ref a: mutable(int)): void
{
    p(a);
    a = a * 2;
}

def g(ref a: const(int)): void
{
    p(a);
}

def h(ref a: immutable(int)): void
{
    p(a);
}

def i(val a: mutable(int)): void
{
    p(a);
}

def j(val a: const(int)): void
{
    p(a);
}

def k(val a: immutable(int)): void
{
    p(a);
}

